  "devDependencies": {
    "@types/express-session": "1.17.0",
    "@types/node": "14.0.27",
    "@types/body-parser": "1.19.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/csurf": "1.9.36",
    "@types/express": "4.17.7",
    "@types/helmet": "0.0.47",
    "@types/node-fetch": "2.5.7",
    "@types/url-join": "4.0.0",
    "@types/uuid": "8.0.0",
