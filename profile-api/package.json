{
  "name": "@datacentricdesign/profile-api",
  "version": "0.0.5",
  "description": "REST API of profile, an Identity and Access Manager (IAM) for the data-centric design tools.",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "start": "npm run migration:run && set debug=* && ts-node-dev --respawn --transpileOnly ./index.ts",
    "prod": "npm run migration:run && tsc && node ./dist/index.js",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./config/ormMigrationWrap.ts",
    "migration:run": "npm run typeorm:cli migration:run",
    "doc": "apidoc -i person/PersonRouter.ts -o dist/public/docs/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datacentricdesign/profile.git"
  },
  "author": "Data-Centric Design Lab <lab@datacentricdesign.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datacentricdesign/profile/issues"
  },
  "homepage": "https://github.com/datacentricdesign/profile#readme",
  "dependencies": {
    "@datacentricdesign/types": "0.0.2",
    "apidoc": "^0.23.0",
    "body-parser": "^1.19.0",
    "class-validator": "^0.12.2",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.1",
    "node-fetch": "^2.6.0",
    "pg": "^8.2.1",
    "postgres": "^1.0.2",
    "typeorm": "^0.2.25",
    "url-join": "^4.0.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/express-session": "^1.17.0",
    "@types/node": "^14.0.27",
    "@types/body-parser": "^1.19.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/csurf": "^1.9.36",
    "@types/express": "^4.17.6",
    "@types/helmet": "0.0.47",
    "@types/node-fetch": "^2.5.7",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.0.0",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.49",
    "typescript": "^3.9.5"
  }
}
