{
  "name": "@datacentricdesign/profile-api",
  "version": "0.1.2",
  "description": "REST API of Profile, an Identity and Access Manager (IAM) for the data-centric design tools.",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only  ./src/index.ts",
    "build": "tsc",
    "typeorm:cli": "node ./node_modules/typeorm/cli -f ./dist/config/ormMigrationWrap.js",
    "migration:run": "npm run typeorm:cli migration:run",
    "prod": "npm run apidoc && ts-node ./src/index.ts",
    "apidoc": "apidoc -i src/ -o dist/public/docs/rest",
    "esdoc": "./node_modules/.bin/esdoc",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "dev": "NODE_ENV=dev nodemon src/index.ts",
    "test": "NODE_ENV=test mocha --check-leaks -r ts-node/register \"src/**/*.spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datacentricdesign/profile.git"
  },
  "author": "Data-Centric Design Lab <lab@datacentricdesign.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datacentricdesign/profile/issues"
  },
  "homepage": "https://github.com/datacentricdesign/profile#readme",
  "dependencies": {
    "@datacentricdesign/types": "0.0.5",
    "@ory/hydra-client": "^1.10.6",
    "apidoc": "0.24.0",
    "body-parser": "1.19.0",
    "class-validator": "^0.13.2",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "csurf": "1.11.0",
    "envalid": "6.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "helmet": "3.23.3",
    "moment": "^2.29.1",
    "node-fetch": "^2.6.6",
    "nodemailer": "^6.7.2",
    "pg": "8.3.0",
    "postgres": "1.0.2",
    "simple-oauth2": "^4.1.0",
    "ts-node": "8.10.2",
    "tslog": "^3.3.0",
    "typeorm": "^0.2.41",
    "typescript": "3.9.7",
    "url-join": "4.0.1",
    "uuid": "8.3.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.0",
    "@types/chai": "^4.2.21",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/csurf": "1.9.36",
    "@types/express": "^4.17.7",
    "@types/express-session": "1.17.0",
    "@types/express-ws": "^3.0.1",
    "@types/fluent-ffmpeg": "^2.1.18",
    "@types/helmet": "0.0.47",
    "@types/jsonwebtoken": "8.5.0",
    "@types/jwk-to-pem": "2.0.0",
    "@types/mocha": "^9.0.0",
    "@types/multer": "^1.4.7",
    "@types/multiparty": "0.0.32",
    "@types/node": "14.0.27",
    "@types/node-fetch": "2.5.7",
    "@types/node-jose": "^1.1.5",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "1.0.4",
    "@types/passport-oauth2-refresh": "1.1.1",
    "@types/pem-jwk": "1.5.0",
    "@types/simple-oauth2": "4.1.0",
    "@types/sinon": "^10.0.2",
    "@types/supertest": "^2.0.11",
    "@types/url-join": "4.0.0",
    "@types/uuid": "8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "chai": "^4.3.4",
    "esdoc": "^0.0.2",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "mocha": "^9.0.2",
    "nodemon": "^2.0.12",
    "sinon": "^11.1.1",
    "supertest": "^6.1.4",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.1.8",
    "tsconfig-paths": "^3.10.1",
    "typescript": "3.9.7"
  }
}
