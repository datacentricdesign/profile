  "license": "MIT",
  "bugs": {
    "url": "https://github.com/datacentricdesign/profile/issues"
  },
  "homepage": "https://github.com/datacentricdesign/profile#readme",
  "dependencies": {
    "@datacentricdesign/types": "0.0.3",
    "apidoc": "0.24.0",
    "body-parser": "1.19.0",
    "class-validator": "0.12.2",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "csurf": "1.11.0",
    "envalid": "6.0.2",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "helmet": "3.23.3",
    "node-fetch": "2.6.0",
    "pg": "8.3.0",
    "postgres": "1.0.2",
    "simple-oauth2": "^4.1.0",
    "ts-node": "8.10.2",
    "typeorm": "0.2.25",
