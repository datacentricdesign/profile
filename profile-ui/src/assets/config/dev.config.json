{
  "name": "DCD Profile",
  "version": "0.1.5",
  "environment": "development",
  "basehref": "/profile",
  "apiURL": "https://dwd.tudelft.nl/profile/api",
  "kratosURL": "http://localhost:4433",
  "authCodeFlow": {
    "issuer": "https://dwd.tudelft.nl:443/",
    "redirectUri": "http://localhost:4200/profile",
    "postLogoutRedirectUri": "http://localhost:4200/profile",
    "clientId": "clients:profile-ui",
    "responseType": "code",
    "scope": "openid profile email offline dcd:persons",
    "useSilentRefresh": false,
    "showDebugInformation": true,
    "sessionChecksEnabled": false,
    "timeoutFactor": 0.01,
    "clearHashAfterLogin": false
  }
}